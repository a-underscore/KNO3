use gdnative::prelude::*;
use lazy_static::lazy_static;
use std::fs;
use super::room::Room;

lazy_static! {
    pub static ref rooms: Vec<Room> = {
        let mut r = Vec::new();

        fs::read_dir("");

        r
    };
}

#[derive(NativeClass)]
#[inherit(Node2D)]
pub struct Rooms;

#[methods]
impl Rooms {
    fn new(_owner: &Node2D) -> Self {
        Self
    }
}
